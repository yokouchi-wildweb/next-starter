{
  "domainConfigVersion": "1.2",
  "singular": "user_tag",
  "plural": "user_tags",
  "label": "ユーザータグ",
  "dbEngine": "Neon",
  "idType": "uuid",
  "relations": [
    {
      "domain": "user",
      "label": "ユーザー",
      "fieldName": "user_ids",
      "fieldType": "uuid",
      "relationType": "belongsToMany",
      "required": false,
      "includeRelationTable": false
    }
  ],
  "useCreatedAt": true,
  "useUpdatedAt": true,
  "fields": [
    {
      "name": "name",
      "label": "タグ名",
      "fieldType": "string",
      "formInput": "textInput",
      "required": true
    },
    {
      "name": "color",
      "label": "カラー",
      "fieldType": "string",
      "formInput": "select",
      "required": false,
      "options": [
        { "value": "primary", "label": "プライマリ" },
        { "value": "secondary", "label": "セカンダリ" },
        { "value": "accent", "label": "アクセント" },
        { "value": "success", "label": "成功色" },
        { "value": "warning", "label": "注意色" },
        { "value": "destructive", "label": "警告色" },
        { "value": "red", "label": "レッド" },
        { "value": "orange", "label": "オレンジ" },
        { "value": "yellow", "label": "イエロー" },
        { "value": "green", "label": "グリーン" },
        { "value": "blue", "label": "ブルー" },
        { "value": "purple", "label": "パープル" },
        { "value": "pink", "label": "ピンク" },
        { "value": "gray", "label": "グレー" }
      ]
    },
    {
      "name": "description",
      "label": "説明",
      "fieldType": "string",
      "formInput": "textarea",
      "required": false
    }
  ],
  "searchFields": ["name", "description"],
  "defaultOrderBy": [["updatedAt", "DESC"]],
  "tableFields": ["name", "color", "description", "updatedAt"],
  "useDetailModal": false,
  "useDuplicateButton": false,
  "useImportExport": false,
  "addToAdminDataMenu": true,
  "useAutoSave": false,
  "generateFiles": {
    "entities": true,
    "components": true,
    "hooks": true,
    "clientServices": true,
    "serverServices": true,
    "fieldConstants": true,
    "adminRoutes": true,
    "registry": true,
    "presenters": true
  }
}
