{
  "singular": "gacha_machine",
  "plural": "gacha_machines",
  "label": "ガチャマシン",
  "dbEngine": "Neon",
  "idType": "uuid",
  "relations": [
  ],
  "useCreatedAt": true,
  "useUpdatedAt": true,
  "fields": [
    {
      "name": "name",
      "label": "マシン名",
      "fieldType": "string",
      "formInput": "textInput",
      "required": true
    },
    {
      "name": "main_image_url",
      "label": "メイン画像",
      "fieldType": "imageUploader",
      "formInput": "imageUploader",
      "required": false,
      "uploadPath": "machine/main",
      "slug": "main"
    },
    {
      "name": "play_cost",
      "label": "プレイコスト(Point)",
      "fieldType": "integer",
      "formInput": "numberInput",
      "required": true
    },
    {
      "name": "sale_start_at",
      "label": "販売開始日",
      "fieldType": "timestamp With Time Zone",
      "formInput": "datetimeInput",
      "required": false
    },
    {
      "name": "sale_end_at",
      "label": "販売終了日",
      "fieldType": "date",
      "formInput": "dateInput",
      "required": false
    },
    {
      "name": "daily_limit",
      "label": "回数制限（1日）",
      "fieldType": "integer",
      "formInput": "numberInput",
      "required": false
    },
    {
      "name": "user_limit",
      "label": "回数制限（ユーザー）",
      "fieldType": "integer",
      "formInput": "numberInput",
      "required": false
    },
    {
      "name": "play_button_type",
      "label": "プレイボタン",
      "fieldType": "array",
      "formInput": "checkbox",
      "required": false,
      "options": [
        {
          "value": "single",
          "label": "1回ガチャ"
        },
        {
          "value": "ten",
          "label": "10回ガチャ"
        },
        {
          "value": "hunndred",
          "label": "100回ガチャ"
        },
        {
          "value": "all",
          "label": "残り全てガチャ"
        }
      ]
    },
    {
      "name": "description",
      "label": "説明",
      "fieldType": "string",
      "formInput": "textarea",
      "required": false
    }
  ],
  "searchFields": [
    "name"
  ],
  "defaultOrderBy": [
    [
      "updatedAt",
      "DESC"
    ]
  ],
  "tableFields": [
    "name",
    "main_image_url",
    "play_cost",
    "sale_start_at",
    "sale_end_at",
    "daily_limit",
    "user_limit"
  ],
  "useDetailModal": true,
  "addToAdminDataMenu": true,
  "generateFiles": {
    "entities": true,
    "components": true,
    "hooks": true,
    "clientServices": true,
    "serverServices": true,
    "fieldConstants": true,
    "adminRoutes": true,
    "registry": true
  }
}
